    "lint:fix": "ng lint --fix",
    "commit": "npx cz",
    "prepare": "husky",
    "pretty-quick:staged": "npx pretty-quick --staged --pattern '**/*.{js,jsx,ts,tsx,html,css,scss,json,md}' --verbose",
